default menu.c32
prompt 0
timeout 50

MENU TITLE Pepsi Challenge Awesome Menu

LABEL CentOS 5
        menu label CentOS 5
        kernel http://mirror.centos.org/centos-5/5.11/os/x86_64/images/pxeboot/vmlinuz
        append ks=http://172.16.179.129/share/ks5.cfg initrd=http://mirror.centos.org/centos-5/5.11/os/x86_64/images/pxeboot/initrd.img ksdevice=bootif ip=dhcp

LABEL CentOS 6
	menu label CentOS 6
        kernel http://mirror.centos.org/centos-6/6.5/os/x86_64/images/pxeboot/vmlinuz unsupported_hardware text
        append ks=http://172.16.179.129/share/ks6.cfg initrd=http://mirror.centos.org/centos-6/6.5/os/x86_64/images/pxeboot/initrd.img ksdevice=bootif ip=dhcp

LABEL reboot
        menu label Reboot
        COM32 reboot.c32
